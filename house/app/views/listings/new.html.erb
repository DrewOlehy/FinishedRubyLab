<h1>New Listing</h1>
<%= form_for :listing, url: listings_path do |f| %>
  <p>
    <%= f.label :address %><br>
    <%= f.text_field :address %>
  </p>

  <p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </p>
 <p>
   <%= f.label :description %><br>
   <%= f.text_area :description %>
 </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
